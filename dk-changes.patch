+  .chat-container {
+    height: 75vh;
+    min-height: 600px;
+  }
+}
+
+@media (min-height: 900px) {
+  .chat-container {
+    height: 80vh;
+    min-height: 700px;
+  }
+}
+
+@media (min-height: 1080px) {
+  .chat-container {
+    height: 80vh;
+    min-height: 800px;
+  }
+}
+
