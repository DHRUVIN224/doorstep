+  box-shadow: 0 2px 5px rgba(0,0,0,0.1);
+  border: 2px solid white;
+}
+
+.message-item {
+  padding: 18px 25px;
+  transition: all 0.2s ease;
+  border-left: 0;
+  border-right: 0;
+}
+
+.message-item:hover {
+  background-color: #f8f9fa;
+  transform: translateY(-1px);
+}
+
+.message-arrow {
+  color: #4a89dc;
+  font-size: 22px;
+}
