+  flex-direction: column;
+  padding: 15px;
+}
+
+.message-card .card-header {
+  background-color: #4a89dc;
+  color: white;
+  padding: 20px 25px;
+  font-weight: 500;
+  font-size: 1.2rem;
+  display: flex;
+  align-items: center;
+  justify-content: flex-start;
+  width: 100%;
+}
+
